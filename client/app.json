{
  "expo": {
    "version": "1.0.0",
    "name": "MovieLibrary",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "slug": "MovieLibrary",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#151515"
    },
    "scheme": "movielib",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#151515"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "A simple way to catalog your movies and TV shows"
  }
}
